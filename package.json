{
  "name": "eth-rub-oracle",
  "version": "0.1.0-canary.1",
  "description": "Реализация ETH/RUB оракула на блокчейне Ethereum.",
  "keywords": [
    "ethereum",
    "eth",
    "solidity",
    "truffle",
    "ganache",
    "oraclize",
    "ruble",
    "rub",
    "рубль",
    "эфириум",
    "эфир"
  ],
  "homepage": "https://github.com/gulitsky/eth-rub-oracle#readme",
  "bugs": "https://github.com/gulitsky/eth-rub-oracle/issues",
  "license": "MIT",
  "author": "Андрей Гулицкий <andrey@gulitsky.me> (https://gulitsky.me)",
  "main": "truffle-config.js",
  "directories": {
    "test": "./test/"
  },
  "repository": "gulitsky/eth-rub-oracle",
  "scripts": {
    "lint": "run-p lint:*",
    "lint:js": "standard --verbose | snazzy",
    "lint:sol": "solium --dir ./contracts/",
    "ganache:run": "docker run --name ganache -d -p 8545:8545 trufflesuite/ganache-cli -a 5",
    "ganache:stop": "docker rm -f ganache",
    "oraclize:run": "docker run --name oraclize -d --network host gulitsky/oraclize -a 4",
    "oraclize:stop": "docker rm -f oraclize",
    "test": "run-s ganache:run oraclize:run oraclize:stop ganache:stop"
  },
  "devDependencies": {
    "dotenv": "^5.0.1",
    "npm-run-all": "^4.1.3",
    "openzeppelin-solidity": "1.9.0",
    "snazzy": "^7.1.1",
    "solium": "^1.1.7",
    "standard": "^11.0.1",
    "truffle": "^4.1.8",
    "truffle-hdwallet-provider": "^0.0.5"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "private": true
}
